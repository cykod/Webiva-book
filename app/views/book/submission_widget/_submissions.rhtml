<% table_for  submissions, ["Page Name","Submitted"], :class => 'mini_table', :width => '100%' do |t| -%>
  <tr>
    <td><%= link_to( h(t.name.to_s),  { :path => [ t.book_book.id ], :version_id => t.id, :controller => 'book/manage', :action => "view_wiki_edits" }, :popup => ['Revision Diff', 'height=680,width=880,scrollbars=yes'] )%><br/>
    Book:<%= link_to h(truncate(t.book_book.name,:length => 40)),  t.book_book.content_admin_url(t.book_page_id)-%>
    </td>
  <td><%= t.created_at.to_s(:short) -%><br/>
  <%= t.created_by ? t.created_by : t.ipaddress %> </td>
</tr>
<% end -%>
